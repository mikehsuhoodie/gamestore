{
    "description": "Standard Game Configuration Specification",
    "structure": {
        "server_script": "game_server.py",
        "client_script": "game_client.py"
    },
    "metadata": {
        "name": "Template Game",
        "version": "2.0.0",
        "type": "GUI",
        "min_players": 2,
        "max_players": 2
    },
    "launch_arguments": {
        "server": [
            "python3",
            "game_server.py",
            "--port",
            "<port>"
        ],
        "client": [
            "python3",
            "game_client.py",
            "--ip",
            "<ip>",
            "--port",
            "<port>"
        ]
    }
}